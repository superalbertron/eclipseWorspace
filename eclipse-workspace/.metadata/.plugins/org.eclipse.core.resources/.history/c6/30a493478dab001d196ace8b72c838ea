package instituto;

public class Profesor extends Persona{

	public Profesor(String nombre, String asignaturas) {
		super(nombre, asignaturas);
		// TODO Esbozo de constructor generado automáticamente
	}

	public void MostrarDatos() {
		System.out.println("Usted es profesor " +getNombre()+ " su asignatura es "+getAsignaturas());
		
	};
	   
	public void Calendario() {
		super.Calendario();
		System.out.println("Bienvenido al curso señor profesor de Howards");
		
	}
	

}
